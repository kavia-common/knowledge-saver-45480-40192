# Copy this file to .env and fill values. Do not commit .env.
# Supabase project URL, e.g. https://<project-ref>.supabase.co
EXPO_PUBLIC_SUPABASE_URL=
# Supabase anon public key from Project Settings > API
EXPO_PUBLIC_SUPABASE_ANON_KEY=
# Used for emailRedirectTo in OTP flows. For Expo, use app scheme:
# clipstack://auth
EXPO_PUBLIC_SITE_URL=clipstack://auth
# Optional: for client-side AI demo only. For production, use server-side.
EXPO_PUBLIC_OPENAI_API_KEY=
